// Q1 a
#include <stdio.h>
int main()
{
    double arr[5],sum=0,mult=1
    int i;
    printf("Enter 5  Elements");
    for(i=0;i<5;i++)
    {
        scanf("%lf",&arr[i]);
        sum+=arr[i];
        mult=mult*arr[i];
    }
    printf("\nSum =%lf",sum);
    printf("\nMultiplication =%lf",mult);
}

// Q1 b
#include<stdio.h>
int main()
{
    int A[100][100],m,n,i,j,sum=0;
    printf("Enter Number of Rows And Columns of Binary Matrix  \n ");
    scanf("%d %d",&m,&n);
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("[%d][%d]",i,j);
            scanf("%d",&A[i][j]);
        }
    }

    for(i=0;i<m;i++)
    {
       sum += A[i][i];
       sum += A[i][n-1-i];
    }
    sum-= A[n/2][n/2];
    printf("\nSum = %d\n",sum);
}

//Q1 C
#include <stdio.h>
int main()
{
    int arr[10],max=0,min=0,i;
    printf("Enter 10  Elements");
    for(i=0;i<10;i++)
    {
        scanf("%d",&arr[i]);
    }
    min=arr[0];
    for(i=1;i<10;i++)
    {   
        if(min > arr[i])
        {
            min=arr[i];
        }
    }
     max=arr[0];
    for(i=1;i<10;i++)
    {   
        if(max < arr[i])
        {
            max=arr[i];
        }
    }
    printf("\n Max = %d \n Min=%d\n",max,min);
    
}

//Q2 a
#include<stdio.h>
int main()
{   int m,n,p,q,i,j,k;
    int A[100][100],B[100][100],C[100][100];
    printf("Enter Number of Rows And Columns of Matrix A \n ");
    scanf("%d %d",&m,&n);
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {   
            printf("[%d][%d]",i,j);
            scanf("%d",&A[i][j]);
        }
    }
    printf("Enter Number of Rows And Columns of Matrix B\n");
    scanf("%d %d",&p,&q);
    if(p==n)
    {
    printf("Enter Elements for matrix B\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {   
            printf("[%d][%d]",i,j);
            scanf("%d",&B[i][j]);
        }
    }
    for(i=0;i<m;i++)
    {
       for(j=0;j<q;j++)
       { C[i][j]=0;
        for(k=0;k<n;k++)
        {
         C[i][j] += A[i][k]*B[k][j];
        }
       }
    }
    printf("Multiplication Matrix C\n");
    for(i=0;i<m;i++)
    {
       for(j=0;j<q;j++)
       {
        printf("%d\t",C[i][j]);
       }
       printf("\n");
    }
    }
    return 0;
} 
//Q2 b
#include<stdio.h>
int main()
{
    int n,i,sum=0,r;
    printf("Enter Number");
    scanf("%d",&n);
    i=n;
    while(n)
    {
       r=n%10;
       n=n/10;
       if(i%r==0)
       {
        sum = r*r;
       }
    }
    if(sum==1)
    {
       printf("True\n");
    }
    else
    {
        printf("False\n");
    }
}

//Q2 c
#include<stdio.h>
int main()
{   int m,n,i,j,max;
    int acc[100][100],C[100];
    printf("Enter Number of Rows And Columns of Matrix Account \n ");
    scanf("%d %d",&m,&n);
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("[%d][%d]",i,j);
            scanf("%d",&acc[i][j]);
        }
    }
    printf("Account Matrix \n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d\t",acc[i][j]);
        }
        printf("\n");
    }
   for(i=0;i<m;i++)
   { C[i]=0;
    for(j=0;j<n;j++)
    {
       C[i] += acc[i][j];
    }
   }
   max=C[0];
    for(i=1;i<m;i++)
    {   
        if(max < C[i])
        {
            max=C[i];
        }
    }
     printf("\n Maximium Wealth = %d\n",max);
}
//Q3 a 
#include<stdio.h>
#include<stdbool.h>
int main()
{
    int arr[100],n,i,count=0,m;
    bool lft,rgt;
    printf("Enter Elements of Flowerbed \n ");
    scanf("%d",&n);
    printf("Enter Count of Plants need to Plant");
    scanf("%d",&m);
    for(i=0;i<n;i++)
    {
            printf("[%d]",i);
            scanf("%d",&arr[i]);
        
    }
    for(i=0;i<n;i++)
    {
        if(arr[i]==0)
        {
             lft=false,rgt=false;
            if(i==0 || arr[i-1]==0)
            lft=true;
            if(i==n-1 || arr[i+1]==0)
            rgt=true;
            if(lft==true && rgt==true)
            {
                arr[i]=1;
                count++;
            }
        }
    }
    printf("Count=%d\n",count);
    if(count>=m)
    printf("True");
    else 
    printf("False");
}

//Q3 b
#include<stdio.h>
int main()
{   int m,n,i,j,k;
    int bin[100][100],C[100][100];
    printf("Enter Number of Rows And Columns of Binary Matrix  \n ");
    scanf("%d %d",&m,&n);
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("[%d][%d]",i,j);
            scanf("%d",&bin[i][j]);
        }
    }
    printf("Binary Matrix\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d\t",bin[i][j]);
        }
        printf("\n");
    }

    for(i=0;i<m;i++)
    {
        for(j=n-1,k=0;j>=0;k++,j--)
        {   
           if(bin[i][k])
           C[i][j]=0;
           else
           C[i][j]=1;
        }   
        printf("\n");
    }
    printf("Output Matrix\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("%d\t",C[i][j]);
        }
        printf("\n");
    }
    
}

//Q3 c
#include<stdio.h>
int main()
{ 
    int m,n,i,j,max,count=0,index;
    int mat[100][100],C[100];
    printf("Enter Number of Rows And Columns of Binary Matrix  \n ");
    scanf("%d %d",&m,&n);
    
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
            printf("[%d][%d]",i,j);
            scanf("%d",&mat[i][j]);
            
        }
    }
     
    for(i=0;i<m;i++)
    {  count=0;
        for(j=0;j<n;j++)
        {
            if (mat[i][j]=1)
            {
                count++;
            }
        }
        C[i]=count;
    }
     max=C[1];
    for(i=2;i<m;i++)
    {   
        if(max < C[i])
        {
            max=C[i];
            
        }
        index=i;
    }
     
    printf("No of Row =%d\n",i);
    printf("Count of ones=%d\n",C[i]);

}
