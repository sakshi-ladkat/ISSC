//Q1 a
#include<stdio.h>
int main()
{
    int a,b,i;
    printf("Enter value of a & b");
    scanf("%d %d",&a,&b);
    printf("%d + %di\n",a,b);
}
//Q1 b
#include<stdio.h>
#include<math.h>
int main()
{ int x,y;
    printf("Enter value of x");
    scanf("%d",&x);
y=((x*x*x)+(2*x*x)+3*(x+0)*(x+1))/sqrt(x*x);
 printf(" f(%d) = %d\n",x,y);
}
#include<stdio.h>
#include<math.h>
int main()
{ int x,y;
    printf("Enter value of x");
    scanf("%d",&x);
y=10+(sqrt((6*(x*x))+(0*x*x*x)+((x+1)*(x)))/2);
 printf(" f(%d) = %d\n",x,y);
}

#include<stdio.h>
int main()
{
    int n,avg=0,i;
    for(i=0;i<10;i++)
    {
        printf("Enter %d th Element",i);
        scanf("%d",&n);
        avg+=i;
    }
   if(avg>50)
   {
     avg=avg/2;
     printf("Avarage = %d\n",avg);
   }
   else
   {
    printf("Average is less than 50\n");
   }
   return 0;
}

#include<stdio.h>
int main()
{
    int n;
    printf("Enter Score value");
    scanf("%d",&n);
    if(n>=90)
    {
        printf("A\n");
    }
    else if(n>=80 && n<90)
    {
         printf("B\n");
    }
    else if(n>=70 && n<80)
    {
         printf("C\n");
    }
    else if(n>=60 && n<70)
    {
        printf("D\n");
    }
  
  
}

#include<stdio.h>
int main()
{
    int x,y,pow=1,i;
    printf("Enter value of x and y");
    scanf("%d %d",&x,&y);
    for(i=0;i<y;i++)
    {
        pow = pow*x;
    }
  printf("%d^%d = %d\n",x,y,pow);
}

#include<stdio.h>
int main()
{
    int n,r,sum=0;
    printf("Enter number");
    scanf("%d",&n);
    while(n>0)
    {  
        r=n%10;
        n=n/10;
        sum +=r;
        if(sum>10)
        {
        n=sum;
        }
        
    }
    
    printf("%d\n",sum);
    }

#include<stdio.h>
int main()
{
    int n,i,sum=0,r;
    printf("Enter Number");
    scanf("%d",&n);
    i=n;
    while(n)
    {
       r=n%10;
       n=n/10;
       if(i%r==0)
       {
        sum = r*r;
       }
    }
    if(sum==1)
    {
       printf("True\n");
    }
    else
    {
        printf("False\n");
    }
}

#include<stdio.h>
int main()
{
    int n,i,count=0,r;
    printf("Enter Number");
    scanf("%d",&n);
    i=n;
    while(n>0)
    {
       r=n%10;
       n=n/10;
       if(i%r==0)
       {
        count ++;
       }
    }
    printf("%d\n",count);
}

